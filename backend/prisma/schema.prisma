// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  id Int @default(autoincrement()) @id
  name String 
  desc String @map("description")
  status Int
  createdAt DateTime
  updatedAt DateTime

  @@map("DriverTask")
}
